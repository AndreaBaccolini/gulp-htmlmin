{
  "name": "gulp-htmlmin",
  "description": "gulp plugin to minify HTML.",
  "version": "1.1.1",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": "jonschlinkert/gulp-htmlmin",
  "license": {
    "type": "MIT",
    "url": "https://github.com/jonschlinkert/gulp-htmlmin/blob/master/LICENSE"
  },
  "keywords": [
    "format",
    "gulp",
    "gulpplugin",
    "htm",
    "html",
    "minifier",
    "minification",
    "minify"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "pretest": "jscs *.js test/*.js && eslint *.js test/*.js",
    "test": "_mocha"
  },
  "dependencies": {
    "bufferstreams": "1.0.1",
    "gulp-util": "^3.0.2",
    "html-minifier": "^0.7.0",
    "object-assign": "^2.0.0",
    "through2": "^0.6.3",
    "tryit": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^2.0.0",
    "eslint": "^0.15.0",
    "jscs": "^1.11.3",
    "mocha": "*",
    "simple-bufferstream": "^0.0.4",
    "vinyl": "^0.4.6"
  },
  "jscsConfig": {
    "preset": "google",
    "disallowMultipleVarDecl": null,
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
